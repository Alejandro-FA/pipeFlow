FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type uniformFixedValue;
        uniformValue sine;
        uniformValueCoeffs
        {
            amplitude 0.00742;
	    	scale     (0 0 1);
            frequency 5.9;
            start     0;
            level     (0 0 0);
        }
    }

    outlet
    {
		type uniformFixedValue;
        uniformValue sine;
        uniformValueCoeffs
        {
            amplitude 0.00742;
	    	scale     (0 0 1);
            frequency 5.9;
            start     0;
            level     (0 0 0);
        }
    }
	
    walls
    {
		type noSlip;  
    }

}

